        <profile>
            <id>hosted</id>
            <activation>
                <property>
                    <name>hosted</name>
                </property>
            </activation>
            <build>
                <plugins>

                    <plugin>
                        <groupId>org.codehaus.mojo</groupId>
                        <artifactId>properties-maven-plugin</artifactId>
                        <version>1.0.2-touk</version>
                        <executions>
                            <execution>
                                <phase>initialize</phase>
                                <goals>
                                    <goal>read-project-properties</goal>
                                </goals>
                                <configuration>
                                    <files>
                                        <file>gwt.properties</file>
                                    </files>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>
                    <plugin>
                        <groupId>org.codehaus.mojo</groupId>
                        <artifactId>exec-maven-plugin</artifactId>
                        <version>1.0.4-touk</version>
                        <executions>
                            <execution>
                                <goals>
                                    <goal>exec</goal>
                                </goals>
                            </execution>
                        </executions>
                        <configuration>
                            <executable>${java.executable}</executable>
                            <arguments>
                                <argument>-Xmx256m</argument>
                                <argument>-Xms128m</argument>
                                <argument>-Xdebug</argument>
                                <argument>-Xrunjdwp:transport=dt_socket,server=y,suspend=n,address=8002</argument>
                                <argument>-classpath</argument>
                                <classpath>
                                    <path>
                                        ${project.basedir}/src/main/java
                                    </path>
                                    <path>
                                        ${project.basedir}/../wdictionary-lib/src/main/java
                                    </path>

                                    <path>${gwt.home.dir}</path>
                                    <path>${gwt.dev.jar}</path>
                                </classpath>
                                <argument>
                                    com.google.gwt.dev.GWTShell
                                </argument>
                                <argument>-out</argument>
                                <argument>
                                    ${project.build.directory}/www
                                </argument>
                                <argument>-noserver</argument>
                                <argument>${app.url}</argument>
                            </arguments>
                        </configuration>
                    </plugin>

                </plugins>
            </build>
        </profile>

        <profile>
            <id>machosted</id>
            <activation>
                <property>
                    <name>machosted</name>
                </property>
            </activation>
            <build>
                <plugins>

                    <plugin>
                        <groupId>org.codehaus.mojo</groupId>
                        <artifactId>properties-maven-plugin</artifactId>
                        <version>1.0.1-touk</version>
                        <executions>
                            <execution>
                                <phase>initialize</phase>
                                <goals>
                                    <goal>read-project-properties</goal>
                                </goals>
                                <configuration>
                                    <files>
                                        <file>gwt.properties</file>
                                    </files>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>

                    <plugin>
                        <groupId>org.codehaus.mojo</groupId>
                        <artifactId>exec-maven-plugin</artifactId>
                        <version>1.0.2-touk</version>
                        <executions>
                            <execution>
                                <goals>
                                    <goal>exec</goal>
                                </goals>
                            </execution>
                        </executions>
                        <configuration>
                            <executable>${java.executable}</executable>
                            <arguments>
                                <argument>-XstartOnFirstThread</argument>
                                <argument>-Xmx1024m</argument>
                                <argument>-Xms1024m</argument>
                                <argument>-Xdebug</argument>
                                <argument>-Xrunjdwp:transport=dt_socket,server=y,suspend=n,address=8002</argument>
                                <argument>-classpath</argument>
                                <classpath>
                                    <path>
                                        ${project.basedir}/src/main/java
                                    </path>
                                    <path>
                                        ${project.basedir}/../wonderful-dictionary-lib/src/main/java
                                    </path>
                                    <path>${gwt.home.dir}</path>
                                    <path>${gwt.dev.jar}</path>

                                </classpath>
                                <argument>
                                    com.google.gwt.dev.GWTShell
                                </argument>
                                <argument>-out</argument>
                                <argument>
                                    ${project.build.directory}/www
                                </argument>
                                <argument>-noserver</argument>
                                <argument>${app.url}</argument>
                            </arguments>
                        </configuration>
                    </plugin>

                </plugins>
            </build>
        </profile>

          <profile>
            <id>hosted2</id>
            <activation>
                <property>
                    <name>hosted2</name>
                </property>
            </activation>
            <build>

                <plugins>

                    <plugin>
                        <groupId>org.codehaus.mojo</groupId>
                        <artifactId>properties-maven-plugin</artifactId>

                        <executions>
                            <execution>
                                <phase>initialize</phase>
                                <goals>
                                    <goal>read-project-properties</goal>
                                </goals>
                                <configuration>
                                    <files>
                                        <file>${project.basedir}/gwt.properties</file>
                                    </files>
                                </configuration>
                            </execution>
                        </executions>

                    </plugin>

                    <plugin>
                        <groupId>org.codehaus.mojo</groupId>
                        <artifactId>exec-maven-plugin</artifactId>
                        <version>1.0.4-touk</version>
                        <executions>
                            <execution>
                                <goals>
                                    <goal>exec</goal>
                                </goals>
                            </execution>
                        </executions>
                        <configuration>
                            <executable>${java.executable}</executable>
                            <arguments>
                                <argument>-Xmx256m</argument>
                                <argument>-Xms128m</argument>
                                <argument>-Xdebug</argument>
                                <argument>-Xrunjdwp:transport=dt_socket,server=y,suspend=n,address=8004</argument>
                                <argument>-classpath</argument>
                                <classpath>
                                    <path>
                                        ${project.basedir}/src/main/java
                                    </path>
                                    <path>
                                        ${project.basedir}/../wonderful-dictionary-lib/src/main/java
                                    </path>
                                    <path>${gwt.home.dir}</path>
                                    <path>${gwt.dev.jar}</path>
                                </classpath>
                                <argument>
                                    com.google.gwt.dev.DevMode
                                </argument>
                                <argument>-noserver</argument>
                                <argument>-gen</argument>
                                <argument>
                                    ${project.build.directory}/review
                                </argument>
                                <argument>-startupUrl</argument>
                                <argument>${app.url}</argument>
                                <!--<argument>-localWorkers</argument>-->
                                <!--<argument>2</argument>-->
                                <argument>-war</argument>
                                <argument>${project.build.directory}/${project.build.finalName}</argument>
                                <argument>-extra</argument>
                                <argument>${project.build.directory}/extra</argument>
                                <argument>pl.touk.top.dictionary.webapp.DictionaryDemoModule</argument>
                            </arguments>
                        </configuration>
                    </plugin>

                </plugins>
            </build>
        </profile>

    </profiles>

