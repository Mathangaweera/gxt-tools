    <profiles>
        <profile>
            <id>skipGwtCompilation</id>
            <activation>
                <property>
                    <name>skipGwtCompilation</name>
                </property>
            </activation>
        </profile>

        <profile>
            <id>gwtCompilation</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <build>
                <finalName>${project.artifactId}</finalName>
                <testResources>
                    <testResource>
                        <directory>src/test/java</directory>
                        <includes>
                            <include>**</include>
                        </includes>
                        <excludes>
                            <exclude>**/*.java</exclude>
                        </excludes>
                    </testResource>
                </testResources>
                <plugins>
                    <plugin>
                        <artifactId>maven-compiler-plugin</artifactId>
                        <inherited>true</inherited>
                        <configuration>
                            <source>1.5</source>
                            <target>1.5</target>
                            <!--<optimise>true</optimise>-->
                            <debug>true</debug>
                        </configuration>
                    </plugin>
                    <plugin>
                        <groupId>org.codehaus.mojo</groupId>
                        <artifactId>gwt-maven-plugin</artifactId>
                        <version>2.0.0</version>
                        <configuration>
                            <className>
                                pl.touk.top.dictionary.webapp.DictionaryDemoModule
                            </className>
                            <outputDirectory>
                                ${project.build.directory}/www
                            </outputDirectory>

                            <style>PRETTY</style>
                            <source>5</source>


                            <jvmOptions>-Djava.awt.headless=true</jvmOptions>
                            <fork>true</fork>
                            <xmx>512m</xmx>
                            <xms>512m</xms>
                        </configuration>

                        <executions>
                            <execution>
                                <goals>
                                    <goal>compile</goal>
                                </goals>
                            </execution>
                        </executions>
                    </plugin>
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-war-plugin</artifactId>
                        <configuration>
                            <webXml>
                                ${basedir}/src/main/webapp/WEB-INF/web.xml
                            </webXml>
                            <warSourceDirectory>
                                ${basedir}/src/main/webapp
                            </warSourceDirectory>
                            <webResources>
                                <resource>
                                    <directory>
                                        ${project.build.directory}/www/pl.touk.top.dictionary.webapp.DictionaryDemoModule
                                    </directory>
                                </resource>
                            </webResources>
                        </configuration>
                    </plugin>
                </plugins>
            </build>
